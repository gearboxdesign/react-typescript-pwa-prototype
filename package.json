{
  "name": "pwa-prototype",
  "version": "1.0.0",
  "description": "Prototype PWA.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "npm run server-compile && node --inspect-brk=5858 index",
    "build": "npm run lint && npm run client-build && npm run server-compile",
    "start": "npm run server-compile && node index",
    "lint": "npm run client-lint && npm run server-lint",
    "client-build": "npm run client-compile && npm run sw-compile",
    "client-compile": "./node_modules/.bin/webpack",
    "client-lint": "./node_modules/.bin/tslint ./client/**/*.ts -c client/tslint.json -p client/tsconfig.json",
    "client-watch": "npm run client-compile -- -w",
    "server-compile": "./node_modules/.bin/tsc -p tsconfig.json",
    "server-lint": "./node_modules/.bin/tslint ./server/**/*.ts -c tslint.json -p tsconfig.json",
    "sw-compile": "./node_modules/.bin/webpack --config webpack.sw.config.js",
    "sw-watch": "npm run sw-compile -- -w"
  },
  "engines": {
    "node": "8.1.2",
    "npm": "5.0.3"
  },
  "author": "Matt Robinson",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "1.16.5",
    "@types/compression": "0.0.34",
    "@types/cors": "2.8.1",
    "@types/helmet": "0.0.36",
    "autoprefixer": "7.1.2",
    "awesome-typescript-loader": "3.2.2",
    "css-loader": "0.28.5",
    "css-mqpacker": "6.0.1",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "node-sass": "4.5.3",
    "postcss-assets": "4.2.0",
    "postcss-inline-svg": "3.0.0",
    "postcss-loader": "2.0.6",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "tslint": "5.5.0",
    "tslint-react": "3.1.0",
    "typescript": "2.4.2",
    "webpack": "3.4.1",
    "webpack-chunk-hash": "0.4.0",
    "webpack-manifest-plugin": "1.2.1"
  },
  "dependencies": {
    "@types/ejs": "2.3.33",
    "@types/express": "4.0.36",
    "@types/lodash": "4.14.71",
    "@types/node": "8.0.17",
    "@types/react": "15.6.0",
    "@types/react-dom": "15.5.1",
    "@types/react-redux": "5.0.1",
    "@types/react-router-dom": "4.0.7",
    "@types/redux-logger": "3.0.0",
    "@types/uuid": "3.4.0",
    "apicache": "1.0.0",
    "app-module-path": "2.2.0",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "cors": "2.8.4",
    "dotenv": "4.0.0",
    "ejs": "2.5.7",
    "express": "4.15.4",
    "helmet": "3.8.1",
    "lodash": "4.17.4",
    "normalize.css": "7.0.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
